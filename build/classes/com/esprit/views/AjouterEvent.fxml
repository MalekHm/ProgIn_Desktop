<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" onKeyTyped="#searchEvent" prefHeight="842.0" prefWidth="1285.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.controllers.AjouterEventController">
   <children>
      <VBox layoutX="77.0" layoutY="28.0" prefHeight="464.0" prefWidth="191.0" spacing="40.0">
         <children>
            <Label text="ID Event" />
            <Label text="Titre" />
            <Label text="Description" />
            <Label text="Date Debut" />
            <Label text="Date Fin" />
            <Label text="Nombre de Personne" />
            <Label text="Prix" />
            <Label text="Categorie" />
         </children>
      </VBox>
      <VBox layoutX="310.0" layoutY="26.0" prefHeight="464.0" prefWidth="262.0" spacing="30.0">
         <children>
            <TextField fx:id="TF_IdEvent" editable="false" prefHeight="33.0" prefWidth="262.0" />
            <TextField fx:id="Tf_Title" onKeyPressed="#testTitle" />
            <TextField fx:id="Tf_Description" onKeyPressed="#testDescription" />
            <DatePicker fx:id="Dp_datedebut" prefHeight="31.0" prefWidth="261.0" />
            <DatePicker fx:id="Dp_enddate" prefHeight="31.0" prefWidth="264.0" />
            <TextField fx:id="Tf_nbPerson" onKeyPressed="#testNbPerson">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
            <TextField fx:id="Tf_Price" onKeyPressed="#testPriceEvent" prefHeight="22.0" prefWidth="262.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
            <ChoiceBox fx:id="CBCategory" prefHeight="31.0" prefWidth="269.0" />
         </children>
      </VBox>
      <TableView fx:id="Tv_Event" layoutX="69.0" layoutY="602.0" onMouseClicked="#selectLine" prefHeight="214.0" prefWidth="1148.0">
         <columns>
            <TableColumn fx:id="Col_Title" prefWidth="121.0" text="Titre" />
            <TableColumn fx:id="Col_Description" prefWidth="374.0" text="description" />
            <TableColumn fx:id="col_StartDate" prefWidth="109.0" text="Date Debut" />
            <TableColumn fx:id="Col_EndDate" prefWidth="95.0" text="Date Fin" />
            <TableColumn fx:id="Col_nbPerson" minWidth="0.0" prefWidth="98.0" text="nb_Personne" />
            <TableColumn fx:id="Col_Price" minWidth="0.0" prefWidth="57.0" text="Prix" />
            <TableColumn fx:id="Col_Id" prefWidth="81.0" text="id_Event" />
            <TableColumn fx:id="Col_nbViews" prefWidth="104.0" text="Nb_Views" />
            <TableColumn fx:id="Col_idCategory" prefWidth="107.0" text="Id_Category" />
         </columns>
      </TableView>
      <Button fx:id="Bt_Ajout" layoutX="669.0" layoutY="28.0" mnemonicParsing="false" onAction="#addEvent" text="Ajouter" />
      <Button fx:id="Bt_Modifier" layoutX="757.0" layoutY="28.0" mnemonicParsing="false" onAction="#modifierEvent" text="Modifier" />
      <Button fx:id="Bt_Supp" layoutX="851.0" layoutY="28.0" mnemonicParsing="false" onAction="#supprimerEvent" text="Supprimer" />
      <TextField fx:id="Tf_SearchEvent" layoutX="512.0" layoutY="548.0" onKeyTyped="#searchEvent" />
      <Label layoutX="293.0" layoutY="553.0" text="Rechercher un evennement :" />
      <ImageView fx:id="imageViewTitre" fitHeight="31.0" fitWidth="30.0" layoutX="583.0" layoutY="88.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="imageViewDescription" fitHeight="31.0" fitWidth="30.0" layoutX="583.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="imageViewNbPerson" fitHeight="31.0" fitWidth="30.0" layoutX="583.0" layoutY="333.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="imageViewPrix" fitHeight="31.0" fitWidth="25.0" layoutX="583.0" layoutY="390.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="eventImage" fitHeight="259.0" fitWidth="366.0" layoutX="750.0" layoutY="174.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="869.0" layoutY="461.0" mnemonicParsing="false" onAction="#OpenFileMan" text="Selectionner une image" />
      <TextField fx:id="Imagepath" layoutX="818.0" layoutY="502.0" opacity="0.0" prefHeight="31.0" prefWidth="278.0" />
   </children>
</AnchorPane>
