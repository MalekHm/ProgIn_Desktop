<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="1017.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.controllers.CategoryEventController">
   <children>
      <VBox layoutX="123.0" layoutY="175.0" prefHeight="155.0" prefWidth="100.0" spacing="43.0">
         <children>
            <Label text="ID" />
            <Label text="Name" />
            <Label text="Description">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Label>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <VBox layoutX="321.0" layoutY="175.0" prefHeight="155.0" prefWidth="183.0" spacing="30.0">
         <children>
            <TextField fx:id="TF_IDCatEvent" editable="false" />
            <TextField fx:id="TF_IDCatNameEvent">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
            <TextField fx:id="TF_IDCatDescEvent" prefHeight="32.0" prefWidth="183.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TextField>
         </children>
      </VBox>
      <Label layoutX="320.0" layoutY="42.0" text="Categorie des evennements">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TableView fx:id="tv_Cat" layoutX="627.0" layoutY="153.0" onMouseClicked="#selectedCategory" prefHeight="200.0" prefWidth="352.0">
        <columns>
          <TableColumn fx:id="Col_idCat" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="Col_NameCat" prefWidth="97.0" text="Name" />
            <TableColumn fx:id="Col_DescCat" prefWidth="179.0" text="Description" />
        </columns>
      </TableView>
      <Button fx:id="bt_Ajout" layoutX="204.0" layoutY="380.0" mnemonicParsing="false" onAction="#ajouterCategory" text="Ajouter" />
      <Button fx:id="bt_Modifier" layoutX="328.0" layoutY="380.0" mnemonicParsing="false" onAction="#modifierCategory" text="Modifier" />
      <Button fx:id="bt_Supprimer" layoutX="460.0" layoutY="380.0" mnemonicParsing="false" onAction="#supprimerCategory" text="Supprimer" />
   </children>
</AnchorPane>
